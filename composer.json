{
    "name": "amirandev/openadmin-tinymce",
    "description": "Integrate TinyMCE editor into Open-Admin",
    "type": "library",
    "keywords": [
        "open-admin",
        "wysiwyg",
        "extension",
        "editor",
        "tinymce"
    ],
    "homepage": "https://github.com/amirandev/openadmin-tinymce",
    "license": "MIT",
    "authors": [
        {
            "name": "amirandev"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=7.0",
        "open-admin-org/open-admin": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Amirandev\\OpenadminTinymce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amirandev\\OpenadminTinymce\\TinymceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
